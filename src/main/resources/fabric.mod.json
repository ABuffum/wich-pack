{
  "schemaVersion": 1,
  "id": "wich",
  "version": "${version}",
  "name": "Wich Pack",
  "description": "Mod Suite for the Ryft SMP. Uses assets from the Squirting Syringe Mod [https://github.com/ImExoOdeex/squirtingsyringe]",
  "authors": [
    "dovewich", "knife_moth", "swordcats", "madkiska", "plushieknight"
  ],
  "contact": {
    "homepage": "wich.fun",
    "sources": ""
  },
  
  "icon": "assets/wich/icon.png",
  
  "accessWidener" : "wich.accesswidener",

  "environment": "*",
  "entrypoints": {
    "main": [
      "fun.wich.ModBase"
    ],
    "client": [
      "fun.wich.ModClient"
    ],
    "fabric-datagen": [
	  "fun.wich.gen.data.ModDatagen"
    ]
  },
  "mixins": [
    "fun.wich.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.14.6",
    "fabric": "*",
    "minecraft": "~1.18.2",
    "java": ">=17"
  },
  "suggests": {
    "another-mod": "*"
  }
}
