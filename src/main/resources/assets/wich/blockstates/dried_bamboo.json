{
  "multipart": [
    {
      "apply": [
        { "model": "wich:block/dried_bamboo1_age0" },
        { "model": "wich:block/dried_bamboo2_age0" },
        { "model": "wich:block/dried_bamboo3_age0" },
        { "model": "wich:block/dried_bamboo4_age0" }
      ],
      "when": { "age": "0" }
    },
    {
      "apply": [
        { "model": "wich:block/dried_bamboo1_age1" },
        { "model": "wich:block/dried_bamboo2_age1" },
        { "model": "wich:block/dried_bamboo3_age1" },
        { "model": "wich:block/dried_bamboo4_age1" }
      ],
      "when": { "age": "1" }
    },
    {
      "apply": { "model": "wich:block/dried_bamboo_small_leaves" },
      "when": { "leaves": "small" }
    },
    {
      "apply": { "model": "wich:block/dried_bamboo_large_leaves" },
      "when": { "leaves": "large" }
    }
  ]
}